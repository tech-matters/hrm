{
  "name": "hrm",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "cd ./dist && sequelize db:migrate && node ./server.js",
    "dev": "tsc -w & cd ./dist && sequelize db:migrate && nodemon ./server.js",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.yaml src/**/*.sql dist/",
    "test": "cd ./dist && RUNNING_TESTS=true jest --passWithNoTests --forceExit",
    "db:create-migration": "cd ./dist && sequelize migration:generate --name ",
    "db:undo-last": "cd ./dist && sequelize db:migrate:undo",
    "lint": "cd ./src && eslint --ext js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tech-matters/hrm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tech-matters/hrm/issues"
  },
  "homepage": "https://github.com/tech-matters/hrm#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.10.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "^1.7.3",
    "js-yaml": "^3.13.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.0",
    "swagger-ui-express": "^4.1.4",
    "twilio-flex-token-validator": "^1.5.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/express": "^4.17.11",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.14.27",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "chalk": "^4.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sequelize-mock": "^0.10.2",
    "supertest": "^4.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5",
    "util": "^0.12.2"
  }
}
